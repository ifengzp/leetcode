{
    "title": "leetcode 刷题",
    "description": "记录LeetCode刷题的过程",
    "author": "aaron",
    "output.name": "site",
    "language": "zh-hans",
    "gitbook": "3.2.3",
    "plugins": [
        "-lunr",
        "-search",
        "search-plus",
        "-highlight",
        "prism",
        "github@^2.0.0",
        "github-buttons@2.1.0",
        "favicon@^0.0.2",
        "todo@^0.1.3",
        "edit-link@^2.0.2",
        "tbfed-pagefooter@^0.0.1",
        "advanced-emoji",
        "splitter",
        "mermaid-gb3",
        "puml",
        "-sharing",
        "sectionx",
        "anchors",
        "copy-code-button",
        "theme-comscore",
        "expandable-chapters",
        "anchor-navigation-ex"
    ],
    "pluginsConfig": {
        "prism": {
            "css": [
                "prismjs/themes/prism-okaidia.css"
            ]
        },
        "github": {
            "url": "https://github.com/ifengzp/leetcode"
        },
        "github-buttons": {
            "repo": "ifengzp/leetcode",
            "types": [
                "star"
            ],
            "size": "small"
        },
        "tbfed-pagefooter": {
            "copyright": "",
            "modify_label": "该文件修订时间：",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        },
        "edit-link": {
            "base": "https://github.com/ifengzp/leetcode/tree/master",
            "label": "编辑本页"
        },
        "favicon": {
            "shortcut": "favicon.ico",
            "bookmark": "favicon.ico"
        },
        "sectionx": {
            "tag": "b"
        },
        "anchor-navigation-ex": {
            "showLevel": true,
            "multipleH1": true,
            "mode": "pageTop",
            "pageTop": {
                "showLevelIcon": true,
                "level1Icon": "fa fa-hand-o-right",
                "level2Icon": "fa fa-hand-o-right",
                "level3Icon": "fa fa-hand-o-right"
            }
        }
    }
}